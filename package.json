{
    "name": "age-ratio",
    "version": "1.0.0",
    "description":
        "Web app that ascertains if two partners are within a suitable age range to date.",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "tests": "jest --watch",
        "test-with-coverage":
            "jest --coverage && cat ./coverage/lcov.info | codacy-coverage --language typescript --accountToken process.env.CODACY_ACCOUNT_TOKEN --username olemak --projectName age-ratio",
        "start": "webpack-dev-server --mode development --open",
        "build": "webpack --mode production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/olemak/age-ratio.git"
    },
    "keywords": ["js", "typescript"],
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
    },
    "author": "Ole Martin Kristiansen",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/olemak/age-ratio/issues"
    },
    "homepage": "https://github.com/olemak/age-ratio#readme",
    "dependencies": {
        "babel-loader": "^7.1.4",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "sanitize-html": "^1.18.2",
        "ts-loader": "^4.1.0",
        "typescript": "^2.8.1",
        "webpack": "^4.5.0"
    },
    "devDependencies": {
        "@types/jest": "^22.2.2",
        "codacy-coverage": "^2.1.1",
        "jest": "^22.4.3",
        "ts-jest": "^22.4.2",
        "webpack-cli": "^2.0.14",
        "webpack-dev-server": "^3.1.1"
    }
}
